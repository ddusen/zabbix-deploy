# 以下为安装 Zabbix 所需的环境变量
## 控制日志颜色：灰色
export CSTART="\033[36m"
export CEND="\033[0m"

## Server 配置
export ServerIP='10.112.100.35'
export ServerHostname='zabbix-server'
export ServerDataPath='/data'

## Zabbix 软件包目录
export ZABBIX_PARCELS="/opt/zabbix-parcels"
