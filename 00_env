# 以下为安装 Zabbix 所需的环境变量
## 控制日志颜色：灰色
export CSTART="\033[36m"
export CEND="\033[0m"

## Server IP
export ServerIP="10.0.0.155"

## Zabbix 软件包目录
export ZABBIX_PARCELS="/opt/zabbix-parcels"

## MySQL
MYSQL_ROOT_PASSWD="234567"
